{
  "name": "vote-role-assignment-bot",
  "version": "0.0.1",
  "description": "Assigns a role automatically provided enough votes",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev --transpile-only src/index.ts",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "build:start": "npm run build ; npm run start:prod",
    "lint": "kcd-scripts lint",
    "lint:staged": "lint-staged --allow-empty",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ezracr",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^13.3.1",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.17",
    "kcd-scripts": "^11.2.2",
    "lint-staged": "^12.1.4",
    "pre-commit": "^1.2.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.ts": [
      "kcd-scripts lint --fix --max-warnings=0",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "engines": {
    "node": ">=16"
  }
}
